{
  "name": "gas-test-sample",
  "version": "0.1.0",
  "description": "Sample of gas-test and gas-test-cli",
  "main": "index.js",
  "dependencies": {
    "browserify": "13.1.0",
    "gasify": "0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "assert": "1.4.1",
    "gas-manager": "0.4.3",
    "gas-test": "0.5.0",
    "gas-test-cli": "0.5.0",
    "mkdirp": "0.5.1",
    "npm-run-all": "3.1.0"
  },
  "scripts": {
    "test:build": "mkdirp dest && gas-test build test/*.js -o dest/test.js",
    "test:upload": "gas upload -c gas-test-credentials.json -e test",
    "pretest": "npm-run-all test:*",
    "test": "gas-test run -c gas-test-credentials.json",
    "prebuild": "mkdirp dest",
    "build": "browserify src/index.js -p gasify -o dest/index.js",
    "upload": "gas upload -c gas-test-credentials.json",
    "deploy": "npm-run-all build upload"
  },
  "keywords": [
    "gas-test",
    "gas-test-cli",
    "Google Apps Script"
  ],
  "author": "fossamagna <fossamagna2@gmail.com>",
  "license": "Apache-2.0"
}
